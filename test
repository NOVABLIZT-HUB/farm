local function setupLabel(label, pos, size, text)
    label.Parent = MainFrame
    label.BackgroundTransparency = 1.000
    label.Position = pos
    label.Size = size
    label.Font = Enum.Font.FredokaOne
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.TextScaled = true
end

