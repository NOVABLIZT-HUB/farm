}

local function mergeConfig(default, user)
	if type(default) ~= "table" then
		return user
	end
	user = user or {}
	for k, v in pairs(default) do
		if type(v) == "table" then
			user[k] = mergeConfig(v, user[k])
		elseif user[k] == nil then
			user[k] = v
		end
	end
	return user
end

